    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@iconscout/react-unicons": "^1.1.6",
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@reduxjs/toolkit": "^1.9.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@uidotdev/usehooks": "^2.4.1",
    "animate.css": "^4.1.1",
    "axios": "^1.2.1",
    "cloudinary-react": "^1.8.1",
    "clsx": "^2.1.1",
    "emoji-mart": "^5.5.2",
    "get-youtube-id": "^1.0.1",
    "lucide-react": "^0.453.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-bootstrap": "2.10.3",
    "react-csv": "^2.2.2",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-fast-compare": "^3.2.2",
    "react-google-recaptcha": "^3.1.0",
    "react-hot-toast": "^2.4.1",
    "react-lazy-load-image-component": "^1.6.0",
    "react-lite-youtube-embed": "^2.4.0",
    "react-markdown": "^9.0.1",
    "react-otp-input": "^3.1.1",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.0",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.6.1",
    "source-map-explorer": "^2.5.3",
    "usehooks-ts": "^3.1.0",
    "wavesurfer.js": "^7.1.1",
    "web-vitals": "^2.1.4",
    "webpack": "^5.91.0",
    "yet-another-react-lightbox": "^3.21.5"
  },
  "scripts": {
    "add-dev": "yarn add --dev --production=false",
    "eject": "react-scripts eject",
    "report": "npm run build --withReport true",
    "dev": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "build": "react-scripts build",
    "start:dev": "webpack-dev-server --mode development --open --hot",
    "build:dev": "webpack --mode production",
    "test": "react-scripts test",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint --ignore-pattern .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix"
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "eslint:recommended",
      "prettier"
    ],
    "rules": {
      "no-console": "off",
      "react/prop-types": "off",
      "no-useless-escape": "off",
      "react/react-in-jsx-scope": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "@eslint/js": "^9.12.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.11.0",
    "husky": "^9.0.11",
    "nano-staged": "^0.8.0",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "rehype-raw": "^7.0.0",
    "tailwindcss": "^3.4.14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": "20",
    "npm": "Please use yarn"
  }
}
